{
  "rabbit_version": "3.12.0",
  "users": [
    {
      "name": "guest",
      "password_hash": "guest",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "guest",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "queues": [
    {
      "name": "ai_processing_queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-max-priority": 5
      }
    },
    {
      "name": "ai_result_queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "notification_queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "ai_summarize_queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-max-priority": 3
      }
    }
  ],
  "exchanges": [
    {
      "name": "smd.direct",
      "vhost": "/",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "smd.topic",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "smd.direct",
      "vhost": "/",
      "destination": "ai_processing_queue",
      "destination_type": "queue",
      "routing_key": "ai.process",
      "arguments": {}
    },
    {
      "source": "smd.direct",
      "vhost": "/",
      "destination": "ai_result_queue",
      "destination_type": "queue",
      "routing_key": "ai.result",
      "arguments": {}
    },
    {
      "source": "smd.topic",
      "vhost": "/",
      "destination": "notification_queue",
      "destination_type": "queue",
      "routing_key": "notification.*",
      "arguments": {}
    },
    {
      "source": "smd.direct",
      "vhost": "/",
      "destination": "ai_summarize_queue",
      "destination_type": "queue",
      "routing_key": "ai.summarize",
      "arguments": {}
    }
  ]
}
